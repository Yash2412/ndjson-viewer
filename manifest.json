{
  "manifest_version": 3,
  "name": "NDJSON Converter",
  "version": "1.0",
  "permissions": ["webRequest", "webNavigation", "storage"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png"
    }
  },
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png"
  },
  "devtools_page": "devtools.html"
}
